{
  "name": "template-nextjs",
  "version": "0.1.0",
  "dependencies": {
    "next": "^13.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@types/node": "^18.11.14",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.29.0",
    "eslint-config-next": "^13.0.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "konk": "^0.27.1",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "typescript": "^4.9.4"
  },
  "private": true,
  "scripts": {
    "build": "next build",
    "check": "konk run c -gL -n'check:*'",
    "check:build": "tsc --noEmit",
    "check:format": "prettier --check .",
    "check:lint": "eslint .",
    "dev": "next dev",
    "fix": "konk run s -L -n'fix:*'",
    "fix:format": "prettier --write . --loglevel warn",
    "lint": "next lint",
    "start": "next start"
  }
}
