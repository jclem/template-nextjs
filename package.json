{
  "name": "template-nextjs",
  "version": "0.1.0",
  "dependencies": {
    "next": "^13.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.2.7"
  },
  "devDependencies": {
    "@types/node": "^18.15.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.35.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "konk": "^0.27.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "typescript": "^4.9.5"
  },
  "private": true,
  "scripts": {
    "build": "next build",
    "check": "konk run c -gL -n'check:*'",
    "check:build": "tsc --noEmit",
    "check:format": "prettier --check .",
    "check:lint": "eslint .",
    "dev": "next dev",
    "fix": "konk run s -L -n'fix:*'",
    "fix:format": "prettier --write . --list-different",
    "lint": "next lint",
    "start": "next start"
  }
}
